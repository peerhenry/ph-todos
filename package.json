{
  "name": "ph-todos",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "build": "webpack",
    "build.watch": "webpack --watch",
    "test.build": "webpack --colors --config webpack-test.config.js",
    "test.build.watch": "npm run test.build -- --watch",
    "test.run": "mocha test-report/testbundle.js --opts test/mocha.opts || exit 0",
    "test.run.watch": "mocha test-report/testbundle.js --opts test/mocha.opts --watch",
    "test.serve": "autoreload-server -w \"**/**.html\" ./test-report 8777",
    "test": "concurrently --kill-others \"npm run test.build.watch\" \"npm run test.run.watch\" \"npm run test.serve\"",
    "test.start": "npm run test.build && startchrome.bat http://localhost:8777/index.html && npm test"
  },
  "author": "PeerHenry",
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "ejs": "^2.5.6",
    "express": "~4.14.1",
    "immutable": "^3.8.1",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "enzyme": "^2.7.1",
    "gulp": "^3.9.1",
    "gulp-run": "^1.7.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "mochawesome": "^2.0.4",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "run-sequence": "^1.2.2",
    "simple-autoreload-server": "^0.2.6",
    "sinon": "^2.0.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}
